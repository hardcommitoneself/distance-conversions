{
    "name": "jeffwhansen/distance-conversions",
    "description": "Distance conversions in PHP",
    "keywords": [
        "jeffwhansen",
        "distance-conversions"
    ],
    "homepage": "https://github.com/jeffwhansen/distance-conversions",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeff Hansen",
            "email": "jhansen@hivalley.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "pestphp/pest": "^1.20",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Jeffwhansen\\DistanceConversions\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jeffwhansen\\DistanceConversions\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
